{
  "manifest_version": 3,
  "name": "Youtube-AIDS",
  "description": "Make youtube worse!",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*",
        "https://www.instagram.com/*",
        "https://example.com/*",
        "https://calnewport.com/*"
      ],
      "js": ["contentScript.js"]
    }
  ]
}
